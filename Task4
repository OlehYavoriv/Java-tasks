public class Task {
    public static void main(String[] args) {
        double m = 140;
        double n = 350;
        double nearestNumber = 0;

        if (m == n)
            System.out.println("Найближчим до 10 є обидва числа m та n: " + m);
        else {

            if (m < n)
                nearestNumber = m;
            else
                nearestNumber = n;

            System.out.println("Найближчим до 10 є число: " + nearestNumber);
        }

    }
}
